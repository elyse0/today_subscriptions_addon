{
  "manifest_version": 2,
  "name": "Only today youtube's subscriptions",
  "version": "1.0.2",
  "description": "Get only your today's subscriptions on your feed",
  "background": {
    "scripts": ["youtube_feed.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube_feed.js"]
    }
  ],
  "permissions": [
    "webNavigation",
    "tabs",
    "alarms",
    "*://*.youtube.com/*"
  ]
}
