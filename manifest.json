{
  "manifest_version": 2,
  "name": "Only today youtube's subscriptions",
  "version": "1.0.6",
  "description": "Get only your today's subscriptions on your feed",
  "background": {
    "scripts": [
      "youtube_feed.js"
    ]
  },
  "options_ui": {
    "page": "settings/options.html"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "alarms",
    "*://www.youtube.com/feed/subscriptions"
  ]
}
